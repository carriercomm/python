    <!--=============================client 配置项================================-->
    <clientConfig name="order">
        <!--ICE STORM-->
        <ice enable="true">
            <configFilePath>/data/www/apps/order-service-server/config/msgPublish.config</configFilePath>
        </ice>
        <!--任务调度-->
        <taskDispath enable="false">
            <xmppConfigFilePath></xmppConfigFilePath>
            <processor></processor>
            <jiraJid></jiraJid>
        </taskDispath>
        <!--key mapping-->
        <keyMapping>
            <systemKeyMappingFilePath></systemKeyMappingFilePath>
        </keyMapping>
    </clientConfig>

    <!--=============================监听器================================-->
    <!--expectclassname固定为com.baoxian.jira.task.TaskBean-->
    <listeners>
        <!--<listener servicename="myservice"-->
        <!--expectclassname="com.baoxian.jira.task.TaskBean"-->
        <!--processor="com.baoxian.jira.common.jiratestmsg"-->
        <!--listenTopic="BBCAR{多方初审任务[未完成(IssueUpdated),处理中(人工)(转呼叫处理)],车险核保任务[未完成(IssueUpdated),处理中(人工)(转呼叫处理,转业管处理)],车险报价任务[报成状态(IssueUpdated)]}">-->
        <!--<properties>-->
        <!--<servicename></servicename>-->
        <!--<port></port>-->
        <!--</properties>-->
        <!--</listener>-->
        <!-- 支付事件监听 -->
        <listener servicename="order-payment-listener"
                  expectclassname="com.baoxian.jira.task.TaskBean"
                  processor="com.baoxian.order.service.jira.listener.JiraTaskEventListener"
                  listenTopic="车险支付任务[支付成功(手动支付成功,自动支付成功),支付放弃(手动支付放弃,自动支付放弃)],车险二次支付任务[支付成功(手动支付成功)]">
        </listener>
        <!-- 承保事件监听 -->
        <listener servicename="order-underwrite-listener"
                  expectclassname="com.baoxian.jira.task.TaskBean"
                  processor="com.baoxian.order.service.jira.listener.JiraTaskEventListener"
                  listenTopic="车险承保任务[承保成功(承保成功),放弃承保(放弃承保)]">
        </listener>
        <!-- 配送事件监听 -->
        <listener servicename="order-delivery-listener"
                  expectclassname="com.baoxian.jira.task.TaskBean"
                  processor="com.baoxian.order.service.jira.listener.JiraTaskEventListener"
                  listenTopic="车险配送任务[快递配送中(快递配送开始),自取配送中(自取配送开始),配送成功(快递配送成功,自取配送成功)]">
        </listener>

        <!-- 打单事件监听 -->
        <listener servicename="order-inspolicyprinting-listener"
                  expectclassname="com.baoxian.jira.task.TaskBean"
                  processor="com.baoxian.order.service.jira.listener.JiraTaskEventListener"
                  listenTopic="车险打单任务[打单成功(打单成功)]">
        </listener>
    </listeners>

