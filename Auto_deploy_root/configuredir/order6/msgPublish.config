#
# This property is used to configure the endpoints of the clock
# subscriber adapter. These endpoints are where the client receives
# topic messages from IceStorm.
#
com.baoxian.sync.Endpoints=tcp:udp

#
# This property is used by the clients to connect to IceStorm.
#
TopicManager.Proxy=BaoxianIceStorm/TopicManager:default -p 10000 -h 10.204.211.93

Ice.ThreadPool.Client.Size=2
Ice.ThreadPool.Client.SizeMax=10
Ice.ThreadPool.Server.Size=5
Ice.ThreadPool.Server.SizeMax=10

#
# Network Tracing
#
# 0 = no network tracing
# 1 = trace connection establishment and closure
# 2 = like 1, but more detailed
# 3 = like 2, but also trace data transfer
#
#Ice.Trace.Network=1
