<?xml version="1.0" encoding="utf-8" ?>
<jiraclient>
    <!--=============================服务端地址信息================================-->
    
	<jiraConfig>
      <!--  <url>http://10.68.14.221:8180/jira/jiraissueservlet</url>-->
        
		<url>http://task.baoxian.in/jira/jiraissueservlet</url>
        
		<username>jira_carbiz3</username>
        
		<jiraRouteUrl>http://task.baoxian.in/jiraRouteServer/jiraRouteSearch</jiraRouteUrl>
    
	</jiraConfig>

    <!--=============================client 配置项================================-->
    
	<clientConfig name="go2">
        <!--ICE STORM-->
       
		<ice enable="false">
            
			<configFilePath>/data/www/ins_share/config/go2/iceStorm.config</configFilePath>
        
		</ice>
        <!--任务调度-->
        <taskDispath enable="false">
            <xmppConfigFilePath></xmppConfigFilePath>
            <processor></processor>
            <jiraJid></jiraJid>
        </taskDispath>
        <!--key mapping-->
        <keyMapping>
            <systemKeyMappingFilePath></systemKeyMappingFilePath>
        </keyMapping>
    </clientConfig>

    <listeners port="9995">
				<!--b2b交易站接收jira核保成功事件 -->

    </listeners>

    <jiraActions>
